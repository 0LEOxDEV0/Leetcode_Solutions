class Solution {
    public int numSubarrayProductLessThanK(int[] nums, int k) {
        if(k<=1) return 0;

        int i=0, j=0;
        int prod= 1, ans=0;
        while(j< nums.length){
            prod*= nums[j];
            j++;
            
            while(prod >= k){
                prod/= nums[i];
                i++;
            }
            ans+= (j-i);
        }
        return ans;
    }
}
